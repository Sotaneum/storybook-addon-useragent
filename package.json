{
  "name": "storybook-addon-useragent",
  "version": "0.2.0",
  "description": "storybook-addon-useragent",
  "keywords": [
    "addon-useragent",
    "storybook-addons"
  ],
  "homepage": "https://Sotaneum.github.io/storybook-addon-useragent",
  "repository": {
    "type": "git",
    "url": "https://github.com/Sotaneum/storybook-addon-useragent"
  },
  "author": "LEE DONGGUN",
  "license": "MIT",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/ts/index.d.ts",
  "files": [
    "dist/**/*",
    "README.md",
    "*.js",
    "*.d.ts"
  ],
  "scripts": {
    "clean": "rimraf ./dist",
    "buildBabel": "concurrently \"npm run buildBabel:cjs\" \"npm run buildBabel:esm\"",
    "buildBabel:cjs": "babel ./src -d ./dist/cjs --extensions \".js,.jsx,.ts,.tsx\"",
    "buildBabel:esm": "babel ./src -d ./dist/esm --env-name esm --extensions \".js,.jsx,.ts,.tsx\"",
    "buildTsc": "tsc --declaration --emitDeclarationOnly --outDir ./dist/ts",
    "prebuild": "npm run clean",
    "build": "concurrently \"npm run buildBabel\" \"npm run buildTsc\"",
    "build:watch": "concurrently \"npm run buildBabel:esm -- --watch\" \"npm run buildTsc -- --watch\"",
    "test": "echo \"Error: no test specified\" && exit 1",
    "storybook": "start-storybook -p 6006",
    "prestart": "rm -rf dist",
    "start": "concurrently \"npm run build:watch\" \"npm run storybook -- --no-manager-cache --quiet\"",
    "build-storybook": "build-storybook",
    "prerelease": "zx scripts/prepublish-checks.mjs",
    "release": "npm run build && auto shipit",
    "eject-ts": "zx scripts/eject-typescript.mjs",
    "predeploy-storybook": "npm run build-storybook",
    "deploy-storybook": "gh-pages -d storybook-static"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.1",
    "@babel/core": "^7.12.3",
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-react": "^7.12.5",
    "@babel/preset-typescript": "^7.13.0",
    "@storybook/addon-essentials": "^6.5.10",
    "@storybook/react": "^6.5.10",
    "@types/react": "^18.0.0",
    "auto": "^10.3.0",
    "babel-loader": "^8.1.0",
    "boxen": "^5.0.1",
    "concurrently": "^6.2.0",
    "dedent": "^0.7.0",
    "gh-pages": "^3.2.3",
    "prettier": "^2.3.1",
    "prop-types": "^15.7.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rimraf": "^3.0.2",
    "typescript": "^4.2.4",
    "zx": "1.14.2"
  },
  "peerDependencies": {
    "@storybook/addons": "^6.3.6 || ^6.5.0",
    "@storybook/api": "^6.3.6 || ^6.5.0",
    "@storybook/components": "^6.3.6 || ^6.5.0",
    "@storybook/core-events": "^6.3.6 || ^6.5.0",
    "@storybook/theming": "^6.3.6 || ^6.5.0",
    "react": "^16.8.0 || ^17.0.0 || ^18.0.0",
    "react-dom": "^16.8.0 || ^17.0.0 || ^18.0.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    },
    "react-dom": {
      "optional": true
    }
  },
  "publishConfig": {
    "access": "public",
    "@sotaneum:registry": "https://npm.pkg.github.com"
  },
  "storybook": {
    "displayName": "UserAgent",
    "supportedFrameworks": [
      "react"
    ],
    "icon": "https://raw.githubusercontent.com/Sotaneum/storybook-addon-useragent/main/docs/assets/logo.png"
  },
  "dependencies": {}
}
